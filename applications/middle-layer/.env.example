# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# Server Configuration
PORT=3001

# MCP Server URL - Options:
# Local: http://localhost:3000/mcp (when running mcp-server locally)
# Cloudflare Workers: https://your-worker.workers.dev/mcp
MCP_SERVER_URL=http://localhost:3000/mcp

# OpenAI Agents Tracing Configuration (Optional)
# Set to 'false' to disable tracing
OPENAI_AGENTS_ENABLE_TRACING=true
# Optional: Set trace level (DEBUG, INFO, WARNING, ERROR)
OPENAI_AGENTS_TRACE_LEVEL=WARNING